{
  "User": {
    "id" : {
      "int": {
        "min": 1,
        "max": 50
      }
    },
    "companyName": {
      "regexp": "[A-Z][a-z]{5,10}"
    },
    "stakeholder": {
    "string": [
      "Thor",
      "Loki",
      "Batman",
      "Superman"
      ]
    },
    "role": {
      "string": [
        "CISO",
        "Head of Security",
        "Head of Engineering",
        "CIO"
      ]
    },
    "email": {
      "regexp": "[a-z]{5}@example\\.com"
    },
    "phone": {
      "regexp": "07[0-10]{9}"
    },
    "NDA": {
      "boolean": {}
    },
    "website": {
    "regexp": "https://[a-z]{5}\\.example\\.com"
    }
  }
}
